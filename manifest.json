{
	"name"			: "Save tweets for later",
	"description"	: "Save tweets for later", 
	"browser_action": {
		"default_icon": {
			"80": "cs.jpg"
		}, 
		"default_title"	: "View saved tweets", 
		"default_popup"	: "index.html"
	},
	"manifest_version"	: 2,
	"version"			: "0.1",
	"content_scripts": [{
		"css"		: [ "main.css" ],
		"js"		: [ "jquery.js", "js/twitter.js" ],
		"matches"	: [ "*://twitter.com/*" ]
	}]
}


